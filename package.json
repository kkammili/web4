{
  "name": "krishnaswebsite",
  "version": "0.1.0",
  "description": "creating some demo components with webpack 4",
  "main": "index.js",
  "author": "",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint"
  },
  "scripts": {
    "start": "concurrently \"npm run run-express-server\" \"yarn webpack-dev-server --content-base dist --hot --mode development --open\"",
    "run-webpack": "webpack --mode development",
    "start-dev": "webpack-dev-server --mode development --open",
    "start-pro": "webpack-dev-server ./src/index.js --output ./foo/index.js --mode production --open",
    "build": "webpack --mode production",
    "run-express-server": "nodemon ./server/server.js"
  },
  "proxy": "http://localhost:3000",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
